algoritmo "semnome"
var
contador,valor:inteiro
inicio
      contador <- 0
      Escreval ("Deseja contar até quanto? ")
      Leia (valor)
      Enquanto (contador <= valor) faca
               Escreval (contador)
               contador <- (contador + 1)
      FimEnquanto
      Escreval ("Terminou a contagem ")
      
fimalgoritmo